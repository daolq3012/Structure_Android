<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fstyle.appname">

    <application
        android:name="com.fstyle.appname.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Structure_Android">
        <activity
            android:name="com.fstyle.appname.screens.main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Structure_Android.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.fstyle.appname.screens.splash.SplashActivity"/>
        <activity android:name="com.fstyle.appname.screens.list_user.ListUserActivity" />
    </application>

</manifest>